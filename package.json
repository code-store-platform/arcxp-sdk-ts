{
  "name": "@code.store/arcxp-sdk-ts",
  "version": "4.21.1",
  "description": "A strongly typed set of ArcXP API's and utilities reduce the amount of work required to develop with ArcXP, starting with reducing the boilerplate code you have to write.",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "repository": {
    "url": "https://github.com/code-store-platform/arcxp-sdk-ts"
  },
  "homepage": "https://github.com/code-store-platform/arcxp-sdk-ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "test": "vitest",
    "gen:ts": "npx ts-node-dev ./src/scripts/json-schema-to-ts.ts",
    "cs": "npx changeset && npx changeset version",
    "swaggerToTypes": "npx swagger-typescript-api -p ./tmp/swagger.json -o ./tmp -n swaggerTypes.ts"
  },
  "keywords": [
    "ArcXP",
    "SDK",
    "Code.Store"
  ],
  "author": "code.store",
  "license": "MIT",
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "axios": "^1.4.0",
    "axios-rate-limit": "^1.3.0",
    "base32-encode": "^1.2.0",
    "form-data": "^4.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/tap": "^15.0.8",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.31.0",
    "eslint-config-custom-server": "*",
    "json-schema-to-typescript": "13.1.1",
    "openapi-typescript": "^6.2.7",
    "tap": "^16.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^0.33.0"
  }
}
